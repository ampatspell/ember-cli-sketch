{
  "name": "ember-cli-sketch",
  "version": "0.1.61",
  "description": "HTML-based design editor addon for Ember.js",
  "keywords": [
    "ember-addon",
    "ember",
    "editor",
    "wysiwyg",
    "drag-n-drop",
    "pdf",
    "design"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ampatspell/ember-cli-sketch.git"
  },
  "license": "MIT",
  "author": "Arnis Vuskans <ampatspell@gmail.com> (http://www.amateurinmotion.com/)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "dist": "ember build -prod",
    "deploy:all": "npm run dist && firebase deploy",
    "deploy:hosting": "npm run dist && firebase deploy --only hosting",
    "tags": "git push --follow-tags",
    "bump": "npm version patch; npm run tags",
    "minor": "npm version minor; npm run tags"
  },
  "dependencies": {
    "broccoli-file-creator": "^2.1.1",
    "broccoli-funnel": "^2.0.2",
    "broccoli-merge-trees": "^3.0.2",
    "ember-cli-babel": "^7.7.3",
    "ember-cli-htmlbars": "^3.0.1",
    "ember-cli-sass": "^10.0.0",
    "sass": "^1.18.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@ember/optional-features": "^0.7.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.9.0",
    "ember-cli-dependency-checker": "^3.1.0",
    "ember-cli-eslint": "^5.1.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-template-lint": "^1.0.0-beta.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.1",
    "ember-resolver": "^5.1.3",
    "ember-source": "~3.9.1",
    "ember-source-channel-url": "^1.1.0",
    "ember-try": "^1.1.0",
    "eslint-plugin-ember": "^6.3.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.1.1",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.8.4"
  },
  "engines": {
    "node": "8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
